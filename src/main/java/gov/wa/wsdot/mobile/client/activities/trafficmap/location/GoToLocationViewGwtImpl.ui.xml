<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget">
    
    <ui:with field="app" type="gov.wa.wsdot.mobile.client.css.AppBundle" />

	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
			<mgwt:center>
				<g:HTML>GoTo Location</g:HTML>
			</mgwt:center>
            <mgwt:right>
                <mgwt:HeaderButton ui:field="doneButton" text="Done" />
            </mgwt:right>
		</mgwt:HeaderPanel>
		
        <mgwt:ScrollPanel ui:field="scrollPanel" scrollingEnabledX="false">
        	<g:FlowPanel ui:field="flowPanel">
            	<mgwt:CellList ui:field="cellList" />
            </g:FlowPanel>
        </mgwt:ScrollPanel>
		
	</mgwt:LayoutPanel>
    
</ui:UiBinder>